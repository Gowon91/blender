    <specifications>
        <specification name="Specification 1: IFCBEAM" ifcVersion="IFC4 IFC4X3_ADD2">
            <applicability minOccurs="0" maxOccurs="unbounded">
                <entity>
                    <name>
                        <simpleValue>IFCBEAM</simpleValue>
                    </name>
                </entity>
            </applicability>
            <requirements>
                <entity instructions="Modelliervorgabe:&#10;Balken/Träger/Unterzüge sind inkl. ihrer Bekleidungen zu modellieren bis zu den relevanten Schichten ab 1cm.&#10;&#10;Erläuterung Elementklasse:&#10;Ein Balken/Unterzug/Träger ist ein horizontales oder nahezu horizontales Strukturelement, das in der Lage ist, Lasten hauptsächlich durch Biegefestigkeit zu widerstehen. Gemäß ÖNORM A 6241-2 Anhang A ist ein Balken/Unterzug/Träger ein sekundäres Bauelement.">
                    <name>
                        <simpleValue>IFCBEAM</simpleValue>
                    </name>
                </entity>
                <property dataType="IFCBOOLEAN" uri="https://identifier.buildingsmart.org/uri/buildingsmart/ifc/4.3/prop/IsExternal" instructions="Angabe, ob dieses Bauteil ein Aussenbauteil ist (JA) oder ein Innenbauteil (NEIN). Als Aussenbauteil grenzt es an den Aussenraum (oder Erdreich, oder Wasser)." cardinality="required">
                    <propertySet>
                        <simpleValue>Pset_BeamCommon</simpleValue>
                    </propertySet>
                    <baseName>
                        <simpleValue>IsExternal</simpleValue>
                    </baseName>
                </property>
                <property dataType="IFCBOOLEAN" uri="https://identifier.buildingsmart.org/uri/buildingsmart/ifc/4.3/prop/LoadBearing" instructions="Angabe, ob dieses Bauteil tragend ist (JA) oder nichttragend (NEIN)" cardinality="required">
                    <propertySet>
                        <simpleValue>Pset_BeamCommon</simpleValue>
                    </propertySet>
                    <baseName>
                        <simpleValue>LoadBearing</simpleValue>
                    </baseName>
                </property>
                <property dataType="IFCLABEL" uri="https://identifier.buildingsmart.org/uri/buildingsmart/ifc/4.3/prop/Status" instructions="Status bzw. Phase des Bauteils insbesondere beim Bauen im Bestand. Neben den Standard-Werten des IFC-Schemas können auch weiterhin die Werte aus der PDF-Datei verwendet werden: &quot;Neu&quot; (new) neues Bauteil als Ergänzung, &quot;Bestand&quot; (existing) bestehendes Bauteil, dass erhalten bleibt, &quot;Abbruch&quot; (demolish) Bauteil, das abgebrochen wird, &quot;Temporär&quot; (temporary) Bauteil und andere Bauelemente, die vorübergehend eingebaut werden (wie Abstützungen, etc.)" cardinality="required">
                    <propertySet>
                        <simpleValue>Pset_BeamCommon</simpleValue>
                    </propertySet>
                    <baseName>
                        <simpleValue>Status</simpleValue>
                    </baseName>
                    <value>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="DEMOLISH" />
                            <xs:enumeration value="EXISTING" />
                            <xs:enumeration value="NEW" />
                            <xs:enumeration value="TEMPORARY" />
                            <xs:enumeration value="OTHER" />
                            <xs:enumeration value="NOTKNOWN" />
                            <xs:enumeration value="UNSET" />
                        </xs:restriction>
                    </value>
                </property>
                <property dataType="IFCLABEL" instructions="Angabe über das (Haupt-)Material eines Elementes (z.B. Beton, Holz, etc.). Wenn ein Element aus mehreren Schichten besteht, ist das Material der Hauptschicht anzugeben (z.B. Aussenwand: Beton)" cardinality="required">
                    <propertySet>
                        <simpleValue>WienBV_BeamSpecific</simpleValue>
                    </propertySet>
                    <baseName>
                        <simpleValue>ElementMainMateriality</simpleValue>
                    </baseName>
                    <value>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="Beton" />
                            <xs:enumeration value="Gipskarton" />
                            <xs:enumeration value="Holz" />
                            <xs:enumeration value="Stahl" />
                            <xs:enumeration value="Ziegel" />
                            <xs:enumeration value="Glas" />
                            <xs:enumeration value="andere" />
                            <xs:enumeration value="ND" />
                            <xs:enumeration value="NE" />
                        </xs:restriction>
                    </value>
                </property>
                <property dataType="IFCLABEL" instructions="Angabe, wie sich die Flammen auf der Oberfläche ausbreiten. Sie wird gemäß den nationalen Bauvorschriften angegeben, die das Brandverhalten von Materialien regeln." cardinality="required">
                    <propertySet>
                        <simpleValue>WienBV_BeamSpecific</simpleValue>
                    </propertySet>
                    <baseName>
                        <simpleValue>SurfaceSpreadOfFlame</simpleValue>
                    </baseName>
                    <value>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="A1" />
                            <xs:enumeration value="A2" />
                            <xs:enumeration value="B" />
                            <xs:enumeration value="C" />
                            <xs:enumeration value="D" />
                            <xs:enumeration value="E" />
                            <xs:enumeration value="F" />
                            <xs:enumeration value="ND" />
                            <xs:enumeration value="NE" />
                        </xs:restriction>
                    </value>
                </property>
                <property dataType="IFCLABEL" instructions="Angabe des 5-stelligen Codes des Aufbaus gem. Aufbautenkatalog. Die Codierung erfolgt über die Angabe der Hauptgruppe (z.B. &quot;AW&quot; für Aussenwände), Trennzeichen (&quot;-&quot;) und der Nummerierung des konkreten Aufbaus." cardinality="required">
                    <propertySet>
                        <simpleValue>WienBV_BeamSpecific</simpleValue>
                    </propertySet>
                    <baseName>
                        <simpleValue>AufbautenNummer</simpleValue>
                    </baseName>
                </property>
            </requirements>
        </specification>
        <specification name="Specification 2: IFCCHIMNEY" ifcVersion="IFC4 IFC4X3_ADD2">
            <applicability minOccurs="0" maxOccurs="unbounded">
                <entity>
                    <name>
                        <simpleValue>IFCCHIMNEY</simpleValue>
                    </name>
                </entity>
            </applicability>
            <requirements>
                <entity instructions="Definition:&#10;Ein Kamin entspricht geometrisch der Definition gem. seiner IFC-Klasse (IFC = Industry Foundation Classes, ISO 16739:2018). Wird der Export der IFC4 - Reference View verwendet, wird der Kamin als IfcChimney übergeben.&#10;Wird der Export der IFC2x3 - Coordination View 2.0 verwendet, wird der Kamin als IfcBuildingElementProxy und mit dem Merkmal &quot;UserdefinedType&quot; = &quot;Abgasanlage&quot; übergeben. &#10;&#10;Hinweis: &#10;Wird ein Kamin (z.B. über Dach) als Rohr geführt, ist dieser als IfcPipeSegment zu übergeben.&#10;&#10;Modelliervorgabe:&#10;Ein Kamin ist als konstruktives Teil zu modellieren, inkl. aller relevanter Schichten ab 1cm.&#10;&#10;Erläuterung Elementklasse:&#10;Kamine/Abgasanlagen sind typischerweise vertikale oder fast vertikale Teile der Konstruktion eines Gebäudes und Teil der Bausubstanz. Oft in Fertigteil- oder Ortbetonbauweise, heute selten in Ziegelbauweise. Ein Kaminrohr ist nicht Teil eines IfcChimneys, sondern per Definition ein Rohr (IfcPipeSegment) und als solches zu modellieren.">
                    <name>
                        <simpleValue>IFCCHIMNEY</simpleValue>
                    </name>
                </entity>
                <property dataType="IFCLABEL" uri="https://identifier.buildingsmart.org/uri/buildingsmart/ifc/4.3/prop/Status" instructions="Status bzw. Phase des Bauteils insbesondere beim Bauen im Bestand. Neben den Standard-Werten des IFC-Schemas können auch weiterhin die Werte aus der PDF-Datei verwendet werden: &quot;Neu&quot; (new) neues Bauteil als Ergänzung, &quot;Bestand&quot; (existing) bestehendes Bauteil, dass erhalten bleibt, &quot;Abbruch&quot; (demolish) Bauteil, das abgebrochen wird, &quot;Temporär&quot; (temporary) Bauteil und andere Bauelemente, die vorübergehend eingebaut werden (wie Abstützungen, etc.)" cardinality="required">
                    <propertySet>
                        <simpleValue>Pset_ChimneyCommon</simpleValue>
                    </propertySet>
                    <baseName>
                        <simpleValue>Status</simpleValue>
                    </baseName>
                    <value>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="DEMOLISH" />
                            <xs:enumeration value="EXISTING" />
                            <xs:enumeration value="NEW" />
                            <xs:enumeration value="TEMPORARY" />
                            <xs:enumeration value="OTHER" />
                            <xs:enumeration value="NOTKNOWN" />
                            <xs:enumeration value="UNSET" />
                        </xs:restriction>
                    </value>
                </property>
                <property dataType="IFCLABEL" instructions="Der vordefinierte bzw benutzerdefinierte Typ der Elementklasse „Bauelement beliebig“ (IfcBuildingElementProxy) wird in diesem Merkmal eingetragen. Der Wert ist je Verwendung des vordefinierten bzw benutzerdefinierten Typs fixiert = vorgegeben (PIP, Verkehrsflaeche, VerkehrsflaecheObjekt, Grundstueck, BauwerksVolumen oder BauwerberObjekt)." cardinality="required">
                    <propertySet>
                        <simpleValue>WienBV_ChimneySpecific</simpleValue>
                    </propertySet>
                    <baseName>
                        <simpleValue>UserDefinedType</simpleValue>
                    </baseName>
                </property>
            </requirements>
        </specification>
        <specification name="Specification 3: IFCCOLUMN" ifcVersion="IFC4 IFC4X3_ADD2">
