        <specification name="Specification 6: IFCDOOR" ifcVersion="IFC4 IFC4X3_ADD2">
            <applicability minOccurs="0" maxOccurs="unbounded">
                <entity>
                    <name>
                        <simpleValue>IFCDOOR</simpleValue>
                    </name>
                </entity>
            </applicability>
            <requirements>
                <entity instructions="Modelliervorgabe:&#10;Türen sind in Wänden verortet und mit geometrischen Angaben zu Architekturmaßen, Durchgangslichten, Flügelteilung und Öffnungsrichtung zu modellieren.&#10;&#10;Erläuterung Elementklasse:&#10;Eine Tür ist ein Bauelement, das überwiegend für den kontrollierten Zugang von Personen und Gütern verwendet wird. Es umfasst Konstruktionen mit Klapp-, Schwenk-, Schiebe- und zusätzlich Dreh- und Klappbetrieb. Eine Tür besteht aus einer Verkleidung und einem oder mehreren Paneelen. Gemäß ÖNORM A 6241-2 Anhang A ist eine Tür ein Verbundelement.">
                    <name>
                        <simpleValue>IFCDOOR</simpleValue>
                    </name>
                </entity>
                <property dataType="IFCLABEL" uri="https://identifier.buildingsmart.org/uri/buildingsmart/ifc/4.3/prop/FireRating" instructions="Feuerwiderstandsklasse gemäß der nationalen oder regionalen Brandschutzverordnung." cardinality="required">
                    <propertySet>
                        <simpleValue>Pset_DoorCommon</simpleValue>
                    </propertySet>
                    <baseName>
                        <simpleValue>FireRating</simpleValue>
                    </baseName>
                    <value>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="ND" />
                            <xs:enumeration value="EI2 30" />
                            <xs:enumeration value="EI2 60" />
                            <xs:enumeration value="EI2 90" />
                            <xs:enumeration value="EI2 30-C" />
                            <xs:enumeration value="EI2 60-C" />
                            <xs:enumeration value="EI2 90-C" />
                            <xs:enumeration value="EI2 30-C-Sm" />
                            <xs:enumeration value="EI2 60-C-Sm" />
                            <xs:enumeration value="EI2 90-C-Sm" />
                            <xs:enumeration value="E 30-C" />
                            <xs:enumeration value="E 60-C" />
                            <xs:enumeration value="E 90-C" />
                            <xs:enumeration value="EI 30" />
                            <xs:enumeration value="EI 60" />
                            <xs:enumeration value="EI 90" />
                            <xs:enumeration value="EI 120" />
                            <xs:enumeration value="EI 180" />
                            <xs:enumeration value="E 30" />
                            <xs:enumeration value="E 60" />
                            <xs:enumeration value="E 90" />
                            <xs:enumeration value="E 120" />
                            <xs:enumeration value="E 180" />
                            <xs:enumeration value="NE" />
                        </xs:restriction>
                    </value>
                </property>
                <property dataType="IFCBOOLEAN" uri="https://identifier.buildingsmart.org/uri/buildingsmart/ifc/4.3/prop/HandicapAccessible" instructions="Hinweis, dass dieses Objekt behindertengerecht gestaltet ist. Auf (TRUE/WAHR) setzen, wenn dieses Element gemäß den örtlichen Bauvorschriften als behindertengerecht eingestuft ist, andernfalls (FALSE/FALSCH)." cardinality="required">
                    <propertySet>
                        <simpleValue>Pset_DoorCommon</simpleValue>
                    </propertySet>
                    <baseName>
                        <simpleValue>HandicapAccessible</simpleValue>
                    </baseName>
                </property>
                <property dataType="IFCBOOLEAN" uri="https://identifier.buildingsmart.org/uri/buildingsmart/ifc/4.3/prop/IsExternal" instructions="Angabe, ob dieses Bauteil ein Aussenbauteil ist (JA) oder ein Innenbauteil (NEIN). Als Aussenbauteil grenzt es an den Aussenraum (oder Erdreich, oder Wasser)." cardinality="required">
                    <propertySet>
                        <simpleValue>Pset_DoorCommon</simpleValue>
                    </propertySet>
                    <baseName>
                        <simpleValue>IsExternal</simpleValue>
                    </baseName>
                </property>
                <property dataType="IFCLABEL" uri="https://identifier.buildingsmart.org/uri/buildingsmart/ifc/4.3/prop/Status" instructions="Status bzw. Phase des Bauteils insbesondere beim Bauen im Bestand. Neben den Standard-Werten des IFC-Schemas können auch weiterhin die Werte aus der PDF-Datei verwendet werden: &quot;Neu&quot; (new) neues Bauteil als Ergänzung, &quot;Bestand&quot; (existing) bestehendes Bauteil, dass erhalten bleibt, &quot;Abbruch&quot; (demolish) Bauteil, das abgebrochen wird, &quot;Temporär&quot; (temporary) Bauteil und andere Bauelemente, die vorübergehend eingebaut werden (wie Abstützungen, etc.)" cardinality="required">
                    <propertySet>
                        <simpleValue>Pset_DoorCommon</simpleValue>
                    </propertySet>
                    <baseName>
                        <simpleValue>Status</simpleValue>
                    </baseName>
                    <value>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="DEMOLISH" />
                            <xs:enumeration value="EXISTING" />
                            <xs:enumeration value="NEW" />
                            <xs:enumeration value="TEMPORARY" />
                            <xs:enumeration value="OTHER" />
                            <xs:enumeration value="NOTKNOWN" />
                            <xs:enumeration value="UNSET" />
                        </xs:restriction>
                    </value>
                </property>
                <property dataType="IFCLABEL" instructions="Das Merkmal „ObjektArt“ beinhaltet für Bauelemente die Angabe, ob das Element Teil einer übergeordneten Art von spezifischen Objekttypus ist. Darunter sind Objekttypen zu verstehen, die in der Summe ihrer modellierten Elemente eine spezifische Ausprägung einnehmen, gem. Auswahl-Set: Schutzdach, Flugdach, Tuervorbau, Freitreppe, Parkeinrichtung, Werbeanlage, Skulptur, Schwimmbecken. Ist das modellierte Element Teil eines solchen größeren Objektes, ist im dazugehörigen Merkmal „ObjektArt“ der entsprechende Wert des Auswahl-Sets einzutragen. Das Merkmal „ObjektArt“ trägt für Bauwerberobjekte, die nicht durch klassische Bauelemente (Wand, Stütze, Träger, etc.) einen übergeordneten spezifischen Objekttypus bilden, sondern entsprechend der Rechtsmaterie eine relevante Fläche darstellen, den entsprechenden Wert gem. Auswahl-Set: BebauteFlaecheOberirdisch, BebauteFlaecheUnterirdisch, VersickerungsoffeneFlaeche,&#10;Giebelflaeche." cardinality="required">
                    <propertySet>
                        <simpleValue>WienBV_BauwerberObjektdaten</simpleValue>
                    </propertySet>
                    <baseName>
                        <simpleValue>ObjektArt</simpleValue>
                    </baseName>
                    <value>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="ND" />
                            <xs:enumeration value="Flugdach" />
                            <xs:enumeration value="Tuervorbau" />
                            <xs:enumeration value="Einfriedung" />
                            <xs:enumeration value="NE" />
                        </xs:restriction>
                    </value>
                </property>
                <property dataType="IFCLENGTHMEASURE" instructions="Angabe der nutzbaren Breite (Durchgangslichte). Ist bei Türen immer einzutragen, bei Fenster nur wenn die Merkmale &quot;FireExit&quot; bzw. &quot;Rettungsweg&quot; auf &quot;WAHR&quot; eingestellt sind." cardinality="required">
                    <propertySet>
                        <simpleValue>WienBV_DoorSpecific</simpleValue>
                    </propertySet>
                    <baseName>
                        <simpleValue>DurchgangslichteBreite</simpleValue>
                    </baseName>
                </property>
                <property dataType="IFCLENGTHMEASURE" instructions="Angabe der nutzbaren Höhe (Durchgangslichte. Ist bei Türen immer einzutragen, bei Fenster nur wenn die Merkmale &quot;FireExit&quot; bzw. &quot;Rettungsweg&quot; auf &quot;WAHR&quot; eingestellt sind." cardinality="required">
                    <propertySet>
                        <simpleValue>WienBV_DoorSpecific</simpleValue>
                    </propertySet>
                    <baseName>
                        <simpleValue>DurchgangslichteHoehe</simpleValue>
                    </baseName>
                </property>
                <property dataType="IFCLABEL" instructions="Angabe des Typs einer Tür. Der Typ kann sein: ein Hauptzugang (Haupt-Eingangstür des Gebäudes), ein HauptzugangBarrierefrei (Angabe in Türen, wenn der Haupteingang nicht barrierefrei erreichbar ist, sondern eine zweite barrierefreie Tür den zweiten Hauptzugang zum Gebäude darstellt), ein Zugang (zu Benützungseinheit/Top Wohnungen, Büro, etc.), ein ZugangBarrierefrei (zu Benützungseinheit/Top Wohnungen, Büro barrierefrei,etc.), oder eine Aufzugtuer (Tür der Schachtwand)." cardinality="required">
                    <propertySet>
                        <simpleValue>WienBV_DoorSpecific</simpleValue>
                    </propertySet>
                    <baseName>
                        <simpleValue>TuerTyp</simpleValue>
                    </baseName>
                    <value>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="ND" />
                            <xs:enumeration value="HauptzugangBarrierefrei" />
                            <xs:enumeration value="Hauptzugang" />
                            <xs:enumeration value="Zugang" />
                            <xs:enumeration value="ZugangBarrierefrei" />
                            <xs:enumeration value="Aufzugstuer" />
                            <xs:enumeration value="NE" />
                        </xs:restriction>
                    </value>
                </property>
                <property dataType="IFCIDENTIFIER" instructions="Angabe der eindeutigen Tür-ID/Tür-Nummer. Codierung erfolgt nach planerseitiger/individueller Vorgabe, nur eine eindeutige fortlaufende Nummer am Ende der Codierung ist einzuhalten." cardinality="required">
                    <propertySet>
                        <simpleValue>WienBV_DoorSpecific</simpleValue>
                    </propertySet>
                    <baseName>
                        <simpleValue>IDTuer</simpleValue>
                    </baseName>
                </property>
                <property dataType="IFCIDENTIFIER" instructions="Angabe in der Tür der eindeutigen Tür-ID/Tür-Nummer aus dem Merkmal &quot;IDTuer&quot; der nächstgelegenen zu erreichenden Fluchttür (z.B. in der Wohnungseingangstür die Tür-ID der Tür die ins Stiegenhaus führt)." cardinality="required">
                    <propertySet>
                        <simpleValue>WienBV_DoorSpecific</simpleValue>
                    </propertySet>
                    <baseName>
                        <simpleValue>IDnaechsteFluchttuer</simpleValue>
                    </baseName>
                </property>
                <property dataType="IFCIDENTIFIER" instructions="Angabe der Top-Nummer einer Nutzungsseinheit. Die Top-Nummer ist nur in Türen zu befüllen, die auch den Zugang oder ZugangBarrierefrei (s. Merkmal TuerTyp) zu einer Nutzungseinheit bilden, in allen anderen Türen verbleibt der Wert bei ND." cardinality="required">
                    <propertySet>
                        <simpleValue>WienBV_DoorSpecific</simpleValue>
                    </propertySet>
                    <baseName>
                        <simpleValue>TopNummer</simpleValue>
                    </baseName>
                </property>
                <property dataType="IFCAREAMEASURE" instructions="Angabe der Nutzfläche der Nutzungsseinheit. Die Top-Nummer ist nur in Türen zu befüllen, die auch die Top-Nummer (s. Merkmal TopNummer) zu einer Nutzungseinheit enthalten, in allen anderen Türen verbleibt der Wert bei 0." cardinality="required">
                    <propertySet>
                        <simpleValue>WienBV_DoorSpecific</simpleValue>
                    </propertySet>
                    <baseName>
                        <simpleValue>TopFlaeche</simpleValue>
                    </baseName>
                </property>
                <property dataType="IFCLABEL" instructions="Angabe des 5-stelligen Codes des Aufbaus gem. Aufbautenkatalog. Die Codierung erfolgt über die Angabe der Hauptgruppe (z.B. &quot;AW&quot; für Aussenwände), Trennzeichen (&quot;-&quot;) und der Nummerierung des konkreten Aufbaus." cardinality="required">
                    <propertySet>
                        <simpleValue>WienBV_DoorSpecific</simpleValue>
                    </propertySet>
                    <baseName>
                        <simpleValue>AufbautenNummer</simpleValue>
                    </baseName>
                </property>
                <property dataType="IFCCOUNTMEASURE" instructions="Angabe über die Verabreichungsplätze im gesamten GastrbBereich. Wert ist nur in der Zugangstüer (s. Merkmal &quot;TuerTyp) des Gastronomiebetriebes zu befüllen." cardinality="required">
                    <propertySet>
                        <simpleValue>WienBV_DoorSpecific</simpleValue>
                    </propertySet>
                    <baseName>
                        <simpleValue>Verabreichungsplaetze</simpleValue>
                    </baseName>
                </property>
                <property dataType="IFCLABEL" instructions="Angabe welche Verglasungsart gem. Auswahl-Set verwendet wird." cardinality="required">
                    <propertySet>
                        <simpleValue>WienBV_DoorSpecific</simpleValue>
                    </propertySet>
                    <baseName>
                        <simpleValue>Verglasungsart</simpleValue>
                    </baseName>
                    <value>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="ND" />
                            <xs:enumeration value="VSG" />
                            <xs:enumeration value="ESG" />
                            <xs:enumeration value="TVG" />
                            <xs:enumeration value="NE" />
                        </xs:restriction>
                    </value>
                </property>
                <property dataType="IFCBOOLEAN" instructions="Angabe, ob die Tür einen Panikbeschlag besitzt (Wert = WAHR) oder nicht (Wert = FALSCH)." cardinality="required">
