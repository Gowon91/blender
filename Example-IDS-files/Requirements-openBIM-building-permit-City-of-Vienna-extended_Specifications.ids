                    </propertySet>
                    <baseName>
                        <simpleValue>Hauptfenster</simpleValue>
                    </baseName>
                </property>
                <property dataType="IFCAREAMEASURE" instructions="Fertigmaß der Öffnung für Belichtungselemente in Wänden oder Dächern, gemessen in der Fassadenebene bzw. in der Ebene der Dachhaut." cardinality="required">
