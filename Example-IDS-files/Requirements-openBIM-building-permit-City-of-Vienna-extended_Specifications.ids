                        <simpleValue>Pset_SpaceOccupancyRequirements</simpleValue>
                    </propertySet>
                    <baseName>
                        <simpleValue>MinimumHeadroom</simpleValue>
                    </baseName>
                </property>
                <property dataType="IFCLABEL" instructions="Angabe, ob eine spezielle Raumwidmung der Rechtsnorm zutrifft gem. des Auswahl-Sets." cardinality="required">
                    <propertySet>
                        <simpleValue>WienBV_SpaceSpecific</simpleValue>
                    </propertySet>
                    <baseName>
                        <simpleValue>WidmungRechtsnorm</simpleValue>
