                            <xs:enumeration value="EXISTING" />
                            <xs:enumeration value="NEW" />
                            <xs:enumeration value="TEMPORARY" />
                            <xs:enumeration value="OTHER" />
                            <xs:enumeration value="NOTKNOWN" />
                            <xs:enumeration value="UNSET" />
                        </xs:restriction>
                    </value>
                </property>
                <property dataType="IFCREAL" instructions="Angabe zur Löschwassermenge des Hydranten in Liter/Minute." cardinality="required">
                    <propertySet>
                        <simpleValue>WienBV_FireSuppressionTerminalTypeSpecific</simpleValue>
                    </propertySet>
                    <baseName>
                        <simpleValue>Wassermenge</simpleValue>
                    </baseName>
                </property>
            </requirements>
        </specification>
