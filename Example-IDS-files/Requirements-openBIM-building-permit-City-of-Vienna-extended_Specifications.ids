                            <xs:enumeration value="VSG" />
                            <xs:enumeration value="ESG" />
                            <xs:enumeration value="TVG" />
                            <xs:enumeration value="NE" />
                        </xs:restriction>
                    </value>
                </property>
                <property dataType="IFCLABEL" instructions="Angabe bezüglich der Ausrichtung des Fensters entsprechend Auswahl-Set (z.B. vertikal, geneigt, horizontal)." cardinality="required">
                    <propertySet>
                        <simpleValue>WienBV_WindowSpecific</simpleValue>
                    </propertySet>
